{
  "name": "openMarket",
  "version": "0.0.1",
  "description": "Desktop application to administrate a generic market with stocks, products, categories etc...",
  "main": "index.js",
  "scripts": {
    "start": "ENV=development electron .",
    "dev": "concurrently -k 'babel-node server.js' 'npm start'",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "test:domain": "./node_modules/.bin/mocha src/test --compilers js:babel-core/register",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --ext=\".js\" src/ test/",
    "build": "webpack --config webpack.config.production.js && electron-packager . OpenMarket --platform=darwin --arch=all --prune --overwrite"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UnexpectedSoftware/openMarket.git"
  },
  "keywords": [
    "market",
    "stocks",
    "desktop",
    "electron"
  ],
  "author": "Victor Caldentey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UnexpectedSoftware/openMarket/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "check-more-types": "^2.21.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "electron-packager": "^6.0.2",
    "electron-prebuilt": "^0.37.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.14.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-electron-launcher": "^0.1.0",
    "karma-jasmine": "^1.0.2",
    "lazy-ass": "^1.4.0",
    "mocha": "^2.5.3",
    "node-localstorage": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "@cycle/dom": "^14.0.0",
    "@cycle/isolate": "^1.4.0",
    "@cycle/rx-run": "^7.0.1",
    "@cycle/xstream-run": "^3.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "lodash": "^4.17.1",
    "node-uuid": "^1.4.7",
    "rx": "^4.1.0",
    "xstream": "^7.0.0"
  }
}
